{
  "name": "le-documentaire",
  "version": "1.0.0",
  "description": "Manage all documents for the workspace in one place.",
  "main": "./bin/www",
  "scripts": {
    "start": "npm-run-all -l -p lint:watch start-server start-client build",
    "start-server": "nodemon",
    "start-client": "npm run build && webpack-dev-server --hot --color",
    "server-seed": "sequelize db:seed:all",
    "build": "webpack -p",
    "lint": "./node_modules/.bin/eslint client/  server/",
    "lint:watch": "watch 'npm run lint' ",
    "coverage": "open coverage/lcov-report/*.html",
    "server-test-path": "NODE_ENV=test istanbul cover _mocha -R test/server/Document.Spec.js test/server/User.Spec.js test/server/Role.Spec.js",
    "test:dev": "sequelize db:seed:all --env=test && npm run server-test-path && npm run coverage",
    "test": "sequelize db:seed:all --env=test && npm run server-test-path ",
    "test:watch": "watch 'npm run test' "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LWanjiru/le-documentaire.git"
  },
  "author": "LWanjiru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LWanjiru/le-documentaire/issues"
  },
  "homepage": "https://github.com/LWanjiru/le-documentaire#readme",
  "devDependencies": {
    "ajv": "^5.2.2",
    "ajv-keywords": "^2.1.0",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.4",
    "file-loader": "^0.11.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "react-hot-api": "^0.5.0",
    "react-hot-loader": "^1.3.1",
    "url-loader": "^0.5.9",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "appcache-webpack-plugin": "^1.3.0",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "jwt-blacklist": "0.0.2",
    "lint": "^1.1.2",
    "materialize-css": "^0.99.0",
    "materialize-loader": "^2.0.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "protractor": "^5.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
