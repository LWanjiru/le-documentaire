{
  "name": "le-documentaire",
  "version": "1.0.0",
  "description": "Manage all documents for the workspace in one place.",
  "main": "./bin/www",
  "scripts": {
    "start": "npm-run-all -l -p lint:watch start-server start-client build",
    "start-server": "nodemon",
    "start-client": "webpack-dev-server --color",
    "build": "webpack -p",
    "lint": "./node_modules/.bin/eslint client/  server/",
    "lint:watch": "watch 'npm run lint' ",
    "test:dev": "NODE_ENV=test istanbul cover _mocha -R server/test/server && open coverage/lcov-report/*.html",
    "test":"NODE_ENV=test mocha test/**",
    "test:watch": "watch 'npm run test",
    "start:dev": "nodemon ./bin/www.js"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LWanjiru/le-documentaire.git"
  },
  "author": "LWanjiru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LWanjiru/le-documentaire/issues"
  },
  "homepage": "https://github.com/LWanjiru/le-documentaire#readme",
  "devDependencies": {
    "ajv": "^5.2.2",
    "ajv-keywords": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jsonwebtoken": "^7.4.1",
    "jwt-blacklist": "0.0.2",
    "lint": "^1.1.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "password-hash": "^1.2.2",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.3.1",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "watch": "^1.0.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "engines": {
    "node": "6.11.1"
  }
}
